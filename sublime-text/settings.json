{
	"theme": "Default Dark.sublime-theme",
	"rulers": [80],
	"font_face": "Menlo",
	"font_options": ["no_italic"],
	"ruler_style": "solid",
	"ruler_width": 1,
	"tab_size": 2,
	"line_numbers": true,
	"gutter": true,
	"margin": 4,
	"fold_buttons": false,
	"auto_hide_tabs": false,
	"auto_hide_status_bar": false,
	"trim_automatic_white_space": true,
	"word_wrap": true,
	"line_padding_top": 2,
	"line_padding_bottom": 2,
	"scroll_past_end": false,
	"draw_indent_guides": true,
	"trim_trailing_white_space_on_save": "all",
	"ensure_newline_at_eof_on_save": true,
	"show_definitions": false,
	"focus_on_file_drop": true,
	"show_tab_close_buttons_on_left": false,
	"file_tab_style": "square",
	"tree_animation_enabled": false,
	"highlight_modified_tabs": true,
	"popup_shadows": false,
	"native_tabs": "preferred",
	"enable_tab_scrolling": false,
	"show_line_endings": false,
	"show_line_column": "disabled",
	"hide_tab_scrolling_buttons": true,
	"hide_new_tab_button": true,
	"show_encoding": false,
	"remember_layout": true,
	"create_window_at_startup": false,
	"show_project_first": true,
	"sublime_merge_path": null,
	"open_files_in_new_window": "never",
	"ignored_packages":
	[
		"Vintage",
	],
	"color_scheme": "Packages/Theme - One Dark/Widget - One Dark.tmTheme",
	"dark_color_scheme": "Monokai.sublime-color-scheme",
	"light_color_scheme": "Mariana.sublime-color-scheme",
}
